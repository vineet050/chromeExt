{
    "name": "Website Blocker",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
      "browser_action": {
          "default_popup": "popup.html"
      },
      "permissions": [
        "tabs",
        "storage"]
  }